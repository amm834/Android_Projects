<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

	<item android:state_pressed="true">
		<shape>

			<solid
				android:color="@color/btn_one_start_color"
			/>
		<stroke
			android:color="@color/primary_color"
			android:width="1dp"
			/>
			<corners
				android:radius="250dp"
			/>

		</shape>
	</item>
	
	
	
	<item>
		<shape>
			
			<gradient
				android:startColor="@color/btn_one_start_color"
				android:endColor="@color/btn_one_end_color"
				android:angle="270"
			/>
			<corners
				android:radius="250dp"
			/>

		</shape>
	</item>

</selector>
